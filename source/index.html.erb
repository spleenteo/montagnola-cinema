<!-- Header 13 -->

<header class="pt-190 pb-130 header_13" style="background-image: url(<%= dato.hero.featured_image.file.fit('crop').width(1900).height(1000).format('pjpg').to_url %>)">

	<!-- Header Menu 13 -->

	<nav class="header_menu_13 pt-30 pb-30 mt-25">
		<div class="container px-xl-0">
			<div class="row justify-content-center">
				<div class="col-xl-10" data-aos-duration="500" data-aos="fade-down" data-aos-delay="500">
					<div class="row justify-content-between align-items-center">
						<div class="col-xl-2 color-white logo">Montagnola da vivere</div>
						<div class="col-xl-8 text-center f-18 medium"></div>
						<div class="col-xl-2 text-right">
							<a href="https://tinyletter.com/comitatogim" target="_blank" class="btn sm action-2 f-16">Newsletter</a>
						</div>
					</div>
				</div>
			</div>
		</div>
	</nav>

	<div class="container px-xl-0 text-center color-white">
		<div class="mb-3 logo d-block d-xl-none color-white logo_mobile">Montagnola da vivere</div>
		<div class="row justify-content-center">
			<div class="col-lg-7" data-aos-duration="500" data-aos="fade-down" data-aos-delay="0">
				<h1 class="big"><%= dato.hero.title %></h1>
			</div>
			<div class="col-lg-6" data-aos-duration="500" data-aos="fade-down" data-aos-delay="250">
				<div class="mt-30 f-22 text-adaptive">
					<%= dato.hero.subtitle %>
				</div>
			</div>
		</div>
	</div>
</header>

<!-- Feature 33 -->

<section class="feature_33 bg-light pt-105 pb-130">
	<div class="container px-xl-0">
		<div class="row">
			<div class="col-xl-1"></div>
			<div class="col-xl-7 col-lg-8">
				<h2 class="small" data-aos-duration="500" data-aos="fade-down" data-aos-delay="0">
					<%= dato.about.title %>
				</h2>
				<div class="mt-25 mb-30 f-22 op-7 color-heading text-adaptive description" data-aos-duration="500" data-aos="fade-down" data-aos-delay="250">
					<%= dato.about.abstract %>
				</div>
			</div>
		</div>
		<div class="row flex-md-row-reverse align-items-end">
			<div class="col-lg-1"></div>
			<div class="col-lg-6 col-md-6 col-sm-6 col-8 text-md-right" data-aos-duration="500" data-aos="fade-down" data-aos-delay="750">
				<img srcset="i/feature_33_img@2x.png 2x" src="i/feature_33_img.png" class="img-fluid img" alt="" />
			</div>

			<div class="col-xl-4 col-lg-5 col-md-6 col-sm-8">
				<%- dato.about.paragraph_block.each do |p| %>
					<div class="mt-55 block" data-aos-duration="500" data-aos="fade-down" data-aos-delay="0">
						<div class="mb-20 f-14 semibold text-uppercase sp-20 title"><%= p.title %></div>
						<div class="color-heading op-7 text-adaptive">
							<%= p.description %>
						</div>
					</div>
				<%- end %>
			</div>
		</div>
	</div>
</section>

<!-- Content 14 -->

<section class="content_14 pt-100 pb-100">
	<div class="container px-xl-0">
		<div class="row justify-content-center">
			<div class="col-xl-8 col-lg-9 col-md-10">
				<div class="pt-55 pb-50 bg-light radius10 text-center inner">
					<div class="row justify-content-center">
						<div class="col-xl-10 col-lg-11 col-md-10 col-11">
							<h2 class="mb-15 small" data-aos-duration="500" data-aos="fade-down" data-aos-delay="0"><%= dato.about.highlight_title %></h2>
							<div class="f-22 color-heading text-adaptive" data-aos-duration="500" data-aos="fade-down" data-aos-delay="250">
								<%= dato.about.highlight_body %>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</section>

<!-- Showcase 9 -->

<section class="showcase_9 bg-light pt-105 pb-90 text-center">
	<div class="container px-xl-0">
		<div class="row justify-content-center">
			<div class="col-xl-8 col-lg-10">
				<h2 class="small" data-aos-duration="500" data-aos="fade-down" data-aos-delay="0"><%= dato.events_page.title %></h2>
				<div class="mt-15 mb-60 f-22 color-heading text-adaptive" data-aos-duration="500" data-aos="fade-down" data-aos-delay="250">
					<%= dato.events_page.abstract %>
				</div>
			</div>
		</div>

		<div class="row align-items-stretch_ text-sm-left">
			<%- dato.events.sort_by(&:data).each do |event| %>
				<div class="mb-30 col-md-4" data-aos-duration="200" data-aos="fade-down" data-aos-delay="0">
					<img style="" src="<%= event.featured_image.file.fit('crop').width(300).height(200).format('pjpg').to_url %>" class="w-full radius10 noradius_bottom" alt="" />
					<div class="w-full pb-30 px-35 radius10 noradius_top block">
						<a href="#" class="mt-25 mb-10 f-22 link color-main"><%= event.title %></a>
						<div class="color-heading text-adaptive">
							<%= event.description %>
						</div>
						<div class="mt-15 f-14 semibold text-uppercase action-2 tags">
							<a href="#" class="link action-2"><%= event.category %></a>
						</div>
						<div class="mt-1 f-14 semibold text-uppercase sp-20 action-2 tags">
							<a href="#" class="link action-2"><%= I18n.l(event.data, format: '%A, %e %B') %></a>,
							<a href="#" class="link action-2"><%= I18n.l(event.data, format: '%H:%M') %></a>
						</div>
					</div>
				</div>
			<% end %>
		</div>

		</div>
	</div>
</section>

